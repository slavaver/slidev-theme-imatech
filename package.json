{
  "name": "slidev-theme-imatech",
  "description": "Theme for university and imatech",
  "version": "0.0.1",
  "author": {
    "name": "Vlad Vereschagin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slavaver/slidev-theme-imatech"
  },
  "keywords": [
    "slidev-theme",
    "slidev",
    "slidev-theme-imatech"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.35.6",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.4",
    "theme-vitesse": "^0.5.2"
  },
  "devDependencies": {
    "@slidev/cli": "^0.35.6",
    "playwright-chromium": "^1.25.2"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "light",
    "highlighter": "shiki",
    "defaults": {
      "aspectRatio": "16/9",
      "canvasWidth": 980,
      "fonts": {
        "sans": "Montserrat",
        "serif": "Roboto Slab",
        "mono": "Roboto Mono"
      },
      "hightlighter": "shiki"
    }
  }
}